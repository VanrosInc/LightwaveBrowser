<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
    <metadata>
        <id>EasyTabs</id>
        <version>1.3.3.0</version>
        <title>EasyTabs</title>
        <authors>Luke Stratman</authors>
        <licenseUrl>https://github.com/lstratman/EasyTabs/blob/master/BSD.txt</licenseUrl>
        <projectUrl>https://github.com/lstratman/EasyTabs</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Library to enable .NET WinForms apps to easily render a set of tabs in their titlebar space, similar to Chrome, Firefox, Trillian, etc.</description>
        <summary />
        <tags>TitleBarTabs</tags>
        <releaseNotes>- Fixed tab window resizing issue when the tab is automatically shown upon another tab closing after an application window resize.</releaseNotes>
        <copyright>(c) 2013-2017, Luke Stratman</copyright>
        <dependencies>
            <dependency id="Win32Interop.Dwmapi" version="1.0.1" />
            <dependency id="Win32Interop.Gdi32" version="1.0.1" />
            <dependency id="Win32Interop.Kernel32" version="1.0.1" />
            <dependency id="Win32Interop.User32" version="1.0.1" />
            <dependency id="Win32Interop.Uxtheme" version="1.0.1" />
        </dependencies>
    </metadata>
    <files>
        <file src="..\bin\Release\Microsoft.WindowsAPICodePack.dll" target="lib\net40\Microsoft.WindowsAPICodePack.dll" />
        <file src="..\bin\Release\Microsoft.WindowsAPICodePack.Shell.dll" target="lib\net40\Microsoft.WindowsAPICodePack.Shell.dll" />
        <file src="..\bin\Release\EasyTabs.dll" target="lib\net40\EasyTabs.dll" />
        <file src="..\bin\Release\EasyTabs.XML" target="lib\net40\EasyTabs.XML" />
    </files>
</package>
